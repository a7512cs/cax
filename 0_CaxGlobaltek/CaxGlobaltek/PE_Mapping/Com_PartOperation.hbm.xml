		</list>
    <list name="comTEMain" lazy="true" cascade="save-update" inverse="true">
			<key column="partOperationSrNo"/>
      <index column="comTEMainIdx"/>
			<one-to-many class="Com_TEMain"/>
		</list>
	</class>
</hibernate-mapping>
